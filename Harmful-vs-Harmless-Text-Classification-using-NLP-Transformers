          "output_type": "display_data"
        }
      ],
      "source": [
        "import numpy as np\n",
        "\n",
        "# Use the already defined top_hate and top_nonhate variables\n",
        "top_harmful_words = top_hate\n",
        "top_harmless_words = top_nonhate\n",
        "\n",
        "# Extract the corresponding scores (coefficients) for harmful words\n",
        "harmful_indices = np.argsort(coefs)[-15:]\n",
        "top_harmful_scores = coefs[harmful_indices][::-1] # Reverse to match the order of top_hate\n",
        "\n",
        "# Extract the corresponding scores (coefficients) for harmless words\n",
        "harmless_indices = np.argsort(coefs)[:15]\n",
        "top_harmless_scores = coefs[harmless_indices]\n",
        "top_df = pd.DataFrame({\n",
        "    'word': np.concatenate([top_harmful_words, top_harmless_words]),\n",
        "    'weight': np.concatenate([top_harmful_scores, top_harmless_scores]),\n",
        "    'label': ['Harmful']*len(top_harmful_words) + ['Harmless']*len(top_harmless_words)\n",
        "})\n",
        "\n",
        "plt.figure(figsize=(10,7))\n",
        "sns.barplot(data=top_df, y='word', x='weight', hue='label', dodge=False)\n",
        "plt.title(\"Most Indicative Words for Each Class\")\n",
        "plt.xlabel(\"Model Weight (Importance)\")\n",
        "plt.ylabel(\"Word\")\n",
        "plt.show()"
      ]
