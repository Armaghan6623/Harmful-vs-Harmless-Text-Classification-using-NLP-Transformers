        "training_args = TrainingArguments(\n",
        "\n",
        "   # eval_strategy=\"steps\",\n",
        "   # evaluation_steps=500,\n",
        "    eval_steps=500,   # Evaluate model performance every 500 training steps.\n",
        "    save_strategy=\"steps\", # Save model based on step intervals (not epochs).\n",
        "    save_steps=1000, # Save a checkpoint every 1000 steps.\n",
        "    per_device_train_batch_size=16, # Number of samples per batch during training (per GPU).\n",
        "    per_device_eval_batch_size=16, # Number of samples per batch during evaluation.\n",
        "    num_train_epochs=3, # Go through the dataset 3 full times.\n",
        "    logging_steps=100, # Log training progress (loss, accuracy) every 100 steps.\n",
        "    learning_rate=2e-5, #Very small updates (so it doesn’t forget what it already knows)\n",
        "    weight_decay=0.01, # Slight regularization to prevent overfitting (keeps weights small).\n",
        ")"
      ]
