        "from lime.lime_text import LimeTextExplainer\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Define class names\n",
        "class_names = ['Harmless', 'Harmful']\n",
        "\n",
        "# Define prediction function\n",
        "predict_fn = lambda x: model.predict_proba(vectorizer.transform(x))\n",
        "\n",
        "# Initialize explainer\n",
        "explainer = LimeTextExplainer(class_names=class_names)\n",
        "\n",
        "# Pick an example\n",
        "sample_text = test_df.iloc[5]['text']\n",
        "print(\"Sample text:\\n\", sample_text)\n",
        "\n",
        "# Generate explanation\n",
        "exp = explainer.explain_instance(sample_text, predict_fn, num_features=10)\n",
        "\n",
        "# Show visualization\n",
        "exp.show_in_notebook(text=sample_text)\n",
        "\n",
        "# You can also save as an image\n",
        "fig = exp.as_pyplot_figure()\n",
        "plt.title(\"LIME Explanation: Harmful vs Harmless\")\n",
        "plt.show()\n"
      ]
