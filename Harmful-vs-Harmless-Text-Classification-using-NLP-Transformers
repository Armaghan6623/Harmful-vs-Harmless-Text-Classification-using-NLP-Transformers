        "***Task 4: Explainability & Visualization***"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "Xy9JGnprAp0F"
      },
      "source": [
        "LIME (Easiest & most student-friendly)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "1aMVwLEPBNzk"
      },
      "outputs": [],
      "source": [
        "# --- LIME INTERPRETABILITY ON DATASET ---\n",
        "\n",
        "!pip install lime -q\n",
        "\n",
        "import torch\n",
        "import random\n",
        "from lime.lime_text import LimeTextExplainer\n",
        "import pandas as pd\n",
        "\n",
        "# ✅ Make sure model is in evaluation mode\n",
        "model.eval()\n",
        "\n",
        "# ✅ Define prediction function for LIME\n",
        "def predict_proba(texts):\n",
        "    \"\"\"Return class probabilities for a list of texts.\"\"\"\n",
        "    inputs = tokenizer(texts, return_tensors=\"pt\", padding=True, truncation=True)\n",
        "    with torch.no_grad():\n",
        "        outputs = model(**inputs)\n",
        "        probs = torch.nn.functional.softmax(outputs.logits, dim=1)\n",
        "    return probs.detach().numpy()\n",
        "\n",
        "# ✅ Initialize LIME explainer\n",
        "explainer = LimeTextExplainer(class_names=[\"Harmless\", \"Harmful\"])\n",
        "\n",
        "# ✅ Choose random samples from your dataset (df)\n",
        "# Make sure df has the text column — adjust column name if needed\n",
        "sample_indices = random.sample(range(len(df)), 3)  # explain 3 random examples\n",
        "\n",
        "for idx in sample_indices:\n",
        "    text = df.iloc[idx]['stemmed_text']  # use your text column\n",
        "    label = df.iloc[idx]['label']\n",
        "    print(f\"\\n📝 Example {idx}: {text}\")\n",
        "    print(f\"True Label: {'Harmful' if label==1 else 'Harmless'}\")\n",
        "\n",
        "    # Generate LIME explanation\n",
        "    exp = explainer.explain_instance(text, predict_proba, num_features=8)\n",
        "    exp.show_in_notebook(text=True)\n"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "xmrTbB-vAyeW"
      },
      "source": [
        "SHAP (more advanced global explanation)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "wM5hS1w_AzRC"
      },
      "outputs": [],
      "source": [
        "# --- SHAP INTERPRETABILITY ---\n",
        "!pip install shap -q\n",
        "\n",
        "import shap\n",
        "\n",
        "model.eval()\n",
        "\n",
        "def predict_proba(texts):\n",
        "    if isinstance(texts, str):\n",
        "        texts = [texts]\n",
        "    inputs = tokenizer(texts, return_tensors=\"pt\", truncation=True, padding=True)\n",
        "    with torch.no_grad():\n",
        "        outputs = model(**inputs)\n",
        "        probs = torch.nn.functional.softmax(outputs.logits, dim=1)\n",
        "    return probs.detach().numpy()\n",
        "\n",
        "# Create explainer\n",
        "explainer = shap.Explainer(predict_proba, shap.maskers.Text(tokenizer))\n",
        "\n",
        "# Example texts\n",
        "texts = [\n",
        "    \"I hate you and your family!\",\n",
        "    \"I hope you have a great day!\"\n",
        "]\n",
        "\n",
        "# Compute SHAP values\n",
        "shap_values = explainer(texts)\n",
        "\n",
        "# Visualize\n",
        "shap.plots.text(shap_values[0])   # Harmful\n",
        "# shap.plots.text(shap_values[1]) # Harmless\n"
      ]
    },
    {
