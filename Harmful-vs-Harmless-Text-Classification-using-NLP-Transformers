        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "import pandas as pd # Added this line\n",
        "\n",
        "# Class balance\n",
        "sns.countplot(x='label', data=df)\n",
        "plt.title(\"Class Balance (0 = Harmless, 1 = Harmful)\")\n",
        "plt.show()\n",
        "\n",
        "# Add the 'stemmed_text' column to df from the processed dataset\n",
        "df['stemmed_text'] = dataset['train']['stemmed_text']\n",
        "\n",
        "# Frequent words\n",
        "cv = CountVectorizer(max_features=20, stop_words='english')\n",
        "word_counts = cv.fit_transform(df['stemmed_text'])\n",
        "words = cv.get_feature_names_out()\n",
        "counts = word_counts.sum(axis=0).A1\n",
        "\n",
        "freq_df = pd.DataFrame({'word': words, 'count': counts}).sort_values(by='count', ascending=False)\n",
        "sns.barplot(y='word', x='count', data=freq_df, palette='viridis')\n",
        "plt.title(\"Top 20 Most Frequent Words\")\n",
        "plt.show()"
      ]
