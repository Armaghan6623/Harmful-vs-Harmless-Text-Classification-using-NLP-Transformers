        "Evaluate TF-IDF + ML Model"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "Wmc5aJEHGQ8i"
      },
      "outputs": [],
      "source": [
        "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score, confusion_matrix\n",
        "\n",
        "# Predict on test set using the Logistic Regression model\n",
        "y_pred = ir_model.predict(X_test)\n",
        "\n",
        "# Calculate metrics\n",
        "tfidf_accuracy  = accuracy_score(y_test, y_pred)\n",
        "tfidf_precision = precision_score(y_test, y_pred)\n",
        "tfidf_recall    = recall_score(y_test, y_pred)\n",
        "tfidf_f1        = f1_score(y_test, y_pred)\n",
        "\n",
        "print(\"TF-IDF + ML Metrics:\")\n",
        "print(f\"Accuracy: {tfidf_accuracy:.4f}\")\n",
        "print(f\"Precision: {tfidf_precision:.4f}\")\n",
        "print(f\"Recall: {tfidf_recall:.4f}\")\n",
        "print(f\"F1-score: {tfidf_f1:.4f}\")\n",
        "\n",
        "# Optional: confusion matrix\n",
        "print(\"Confusion Matrix:\\n\", confusion_matrix(y_test, y_pred))"
      ]
    },
