      ],
      "source": [
        "#Stemming\n",
        "\n",
        "from nltk.stem import PorterStemmer, WordNetLemmatizer\n",
        "from nltk.tokenize import word_tokenize\n",
        "\n",
        "stemmer = PorterStemmer()\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "\n",
        "def stem_text(text):\n",
        " tokens = word_tokenize(text)\n",
        " stemmed = [stemmer.stem(word) for word in tokens]\n",
        " return \" \".join(stemmed)\n",
        "\n",
        " #Apply stemming to the 'no_punct_text' column in the dataset object\n",
        "dataset = dataset.map(lambda example: {'stemmed_text': stem_text(example['no_punct_text'])})\n",
        "\n",
        " #Print a preview to verify\n",
        "print(\"Original text (no punctuation):\", dataset['train'][0]['no_punct_text'])\n",
        "print(\"Stemmed text:\", dataset['train'][0]['stemmed_text'])\n"
      ]
    },
